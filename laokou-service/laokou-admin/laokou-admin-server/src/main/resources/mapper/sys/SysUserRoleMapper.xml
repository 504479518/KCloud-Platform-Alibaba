<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.laokou.admin.server.domain.sys.repository.mapper.SysUserRoleMapper">
    <insert id="insertBatch" useGeneratedKeys="false">
        insert into boot_sys_user_role(role_id,user_id)
        values
        <foreach collection="list" item="item" separator=",">
            (#{item.roleId},#{item.userId})
        </foreach>
    </insert>
</mapper>