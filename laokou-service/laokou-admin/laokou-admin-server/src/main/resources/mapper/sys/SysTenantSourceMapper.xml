<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.laokou.admin.server.domain.sys.repository.mapper.SysTenantSourceMapper">

    <select id="getVersion" resultType="integer">
        select `version` from boot_sys_tenant_source where id = #{id}
    </select>

    <select id="queryTenantSourcePage" resultType="org.laokou.admin.client.vo.SysTenantSourceVO">
        select `name`,driver_class_name,url from boot_sys_tenant_source
    </select>

</mapper>
